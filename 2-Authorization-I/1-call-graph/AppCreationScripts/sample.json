{
    "Sample": {
        "Title": "Angular single-page application using MSAL Angular to sign-in users with Azure Active Directory and call the Microsoft Graph API",
        "Level": 100,
        "Client": "Angular SPA",
        "Service": "Microsoft Graph",
        "RepositoryUrl": "ms-identity-javascript-angular-tutorial",
        "Endpoint": "AAD v2.0",
        "Languages": ["javascript", "angular"],
        "Products": ["azure-active-directory", "ms-graph", "msal-angular"],
        "description": "This sample demonstrates an Angular single-page application (SPA) that lets users sign-in with Azure Active Directory (Azure AD) using the Microsoft Authentication Library for Angular (MSAL Angular)"
    },
    "AADApps": [
        {
            "Id": "spa",
            "Name": "msal-angular-spa",
            "Kind": "SinglePageApplication",
            "HomePage": "http://localhost:4200/",
            "ReplyUrls": "http://localhost:4200/",
            "Audience": "AzureADMyOrg",
            "Sample": {
                "SampleSubPath": "2-Authorization-I\\1-call-graph",
                "ProjectDirectory": "\\SPA"
            },
            "RequiredResourcesAccess": [
                {
                    "Resource": "Microsoft Graph",
                    "DelegatedPermissions": ["User.Read", "Contacts.Read"]
                }
            ]
        }
    ],
    "CodeConfiguration": [
        {
            "App": "spa",
            "SettingKind": "Replace",
            "SettingFile": "\\..\\SPA\\src\\app\\auth-config.ts",
            "Mappings": [
                {
                    "key": "Enter_the_Application_Id_Here",
                    "value": ".AppId"
                },
                {
                    "key": "Enter_the_Tenant_Info_Here",
                    "value": "$tenantId"
                }
            ]
        }
    ]
}