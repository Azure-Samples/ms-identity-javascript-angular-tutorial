{
    "Sample": {
      "Title": "Angular single-page application using MSAL Angular to sign-in users against Azure Active Directory",
      "Level": 100,
      "Client": "Angular SPA",
      "RepositoryUrl": "ms-identity-javascript-angular-tutorial",
      "Endpoint": "AAD v2.0"
    },
    "AADApps": [
      {
        "Id": "spa",
        "Name": "msal-angular-spa",
        "Kind" :  "SinglePageApplication",
        "Audience": "AzureADMyOrg",
        "HomePage": "http://localhost:4200",
        "ReplyUrls":"http://localhost:4200",
        "ManualSteps": [
            {
                "Comment": "Navigate to the Manifest page, find the 'replyUrlsWithType' section and change the type of redirect URI to 'Spa'"
            }
        ]
      }
    ],
  
    "CodeConfiguration": [
      {
        "App": "spa",
        "SettingKind": "Replace",
        "SettingFile": "\\..\\App\\src\\app\\app-config.json",
        "Mappings": [
          {
            "key": "Enter_the_Application_Id_Here",
            "value": ".AppId"
          },
          {
            "key": "Enter_the_Cloud_Instance_Id_Here/Enter_the_Tenant_Info_Here",
            "value": "\"https://login.microsoftonline.com/\"+$tenantName"
          }
        ]
      }
    ]
  }