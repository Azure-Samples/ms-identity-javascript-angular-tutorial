{
    "Sample": {
        "Author": "derisen",
        "Title": "Angular single-page application using MSAL Angular to sign-in users against Azure AD B2C",
        "Level": 100,
        "Client": "Angular SPA",
        "RepositoryUrl": "ms-identity-javascript-angular-tutorial",
        "Endpoint": "AAD v2.0",
        "Description": "Angular single-page application using MSAL Angular to sign-in users against Azure AD B2C",
        "Languages": [
            "javascript",
            "typescript"
        ],
        "Products": [
            "azure-active-directory-b2c",
            "msal-js",
            "msal-angular"
        ],
        "Platform": "JavaScript",
        "Provider": "B2C"
    },
    "AADApps": [
        {
            "Id": "client",
            "Name": "msal-angular-spa",
            "SampleSubPath": "1-Authentication\\2-sign-in-b2c\\SPA",
            "Kind": "SinglePageApplication",
            "Audience": "AzureADandPersonalMicrosoftAccount",
            "HomePage": "http://localhost:4200",
            "ReplyUrls": "http://localhost:4200, http://localhost:4200/auth"
        }
    ],
    "CodeConfiguration": [
        {
            "App": "client",
            "SettingKind": "Replace",
            "SettingFile": "\\..\\SPA\\src\\app\\auth-config.ts",
            "Mappings": [
                {
                    "key": "Enter_the_Application_Id_Here",
                    "value": ".AppId"
                },
                {
                    "key": "Enter_the_Tenant_Info_Here",
                    "value": "$tenantName"
                }
            ]
        }
    ]
}